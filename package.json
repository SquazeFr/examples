{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "deploy": "remix build && wrangler deploy",
    "dev": "remix dev --no-restart -c \"npm start\"",
    "start": "wrangler dev ./build/index.js",
    "typecheck": "tsc",
    "generate": "drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./drizzle/schema.ts",
    "migrate": "tsx drizzle/migrate",
    "studio": "drizzle-kit studio --port 3333",
    "db:seed": "tsx drizzle/seed"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@libsql/client": "^0.3.0",
    "@remix-run/cloudflare": "^1.18.1",
    "@remix-run/css-bundle": "^1.18.1",
    "@remix-run/eslint-config": "^1.17.0",
    "@remix-run/react": "^1.18.1",
    "@shopify/eslint-plugin": "^42.1.0",
    "@shopify/prettier-config": "^1.1.2",
    "bcryptjs": "^2.4.3",
    "drizzle-orm": "^0.27.2",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.18.1",
    "@remix-run/eslint-config": "^1.18.1",
    "@shopify/eslint-plugin": "^42.1.0",
    "@shopify/prettier-config": "^1.1.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.2",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.5",
    "eslint": "^8.38.0",
    "eslint-plugin-hydrogen": "^0.12.3",
    "lorem-ipsum": "^2.0.8",
    "tailwindcss": "^3.3.3",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "wrangler": "^3.1.1"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
