{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "typecheck": "tsc",
    "generate": "pnpm drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./drizzle/schema.ts",
    "migrate": "tsx drizzle/migrate",
    "seed": "tsx drizzle/seed"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@libsql/client": "^0.2.0",
    "@remix-run/cloudflare": "^1.17.0",
    "@remix-run/cloudflare-workers": "^1.17.0",
    "@remix-run/css-bundle": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "@shopify/hydrogen": "^2023.4.3",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "drizzle-orm": "^0.26.5",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typographic-base": "^1.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.17.0",
    "@remix-run/eslint-config": "^1.17.0",
    "@shopify/eslint-plugin": "^42.1.0",
    "@shopify/prettier-config": "^1.1.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.40.0",
    "@types/node": "^20.3.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.1.4",
    "drizzle-kit": "^0.18.1",
    "drizzle-zod": "^0.4.3",
    "eslint": "^8.38.0",
    "eslint-plugin-hydrogen": "^0.12.3",
    "lorem-ipsum": "^2.0.8",
    "miniflare": "^2.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "wrangler": "^2.15.1"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
